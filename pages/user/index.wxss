page {
  background-color: #efeff4;
}
.user_info_wrap .user_info {
  height: 45vh;
  overflow: hidden;
  position: relative;
}
.user_info_wrap .user_info .user_img_wrap {
  position: relative;
}
.user_info_wrap .user_info .user_img_wrap .user_big_icon {
  height: 50vh;
  width: 100%;
  filter: blur(10px);
}
.user_info_wrap .user_info .user_img_wrap .user_little {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 20%;
  text-align: center;
}
.user_info_wrap .user_info .user_img_wrap .user_little .user_little_icon {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.user_info_wrap .user_info .user_img_wrap .user_little .user_name {
  color: #fff;
  margin-top: 25rpx;
  font-size: 35rpx;
}
.user_info_wrap .user_info .user_btn {
  height: 100%;
  width: 100%;
}
.user_info_wrap .user_info .user_btn button {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 40%;
  color: var(--themeColor);
  border: 1rpx solid currentColor;
  font-size: 38rpx;
  padding: 30rpx;
  border-radius: 15rpx;
}
.version_wrap {
  height: 100%;
  display: flex;
  box-sizing: border-box;
  padding: 50rpx;
  justify-content: space-between;
  align-items: center;
}
.version_wrap .header {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}
.version_wrap .header .h1 {
  font-weight: bold;
  margin-bottom: 20rpx;
}
.version_wrap .header .h1content {
  font-size: 12px;
}
.version_wrap .version {
  font-weight: 600;
  font-size: 30rpx;
  margin-right: 20rpx;
}
.version_wrap .section {
  margin: 20rpx 0;
  height: 120rpx;
  line-height: 120rpx;
  border-radius: 10rpx;
  background: #fff;
  padding: 0 20rpx;
}
.version_wrap .section .button {
  display: flex;
  justify-content: space-between;
}
/* 用户功能 */
.function {
  display: flex;
  flex-direction: column;
}
/* 意见反馈 and 用户功能 */
.item {
  padding-left: 0;
  padding-right: 0;
  line-height: inherit;
  background-color: #f8f8f8;
  display: flex;
  flex-direction: row;
  height: 90rpx;
  align-items: center;
  margin: 10rpx 50rpx;
  text-align: left;
  text-decoration: none;
  border-radius: 10rpx;
  border: 0rpx;
  justify-content: space-between;
}
.item .item_left {
  margin-left: 10rpx;
}
.item .item_left .item_title {
  margin-left: 10rpx;
  font-size: 32rpx;
}
.item .item_right {
  font-weight: 600;
  font-size: 30rpx;
  margin-right: 20rpx;
}
.item::after {
  border: none;
  border-radius: 0;
}
