.container {
  background-color: #ff9b6e;
  height: 90vh;
  margin-top: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 5%;
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
}
.container .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 20rpx;
  height: 80rpx;
  font-size: 32rpx;
  background-color: #f8f8f8;
}
.container .item .name_wrap {
  display: flex;
  font-size: 35rpx;
  margin-left: 20rpx;
}
.container .item .name_wrap .min_icon {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: var(--themeColor);
}
.container .item .name_wrap .text {
  margin-left: 5rpx;
}
.container .item .iconfont {
  font-size: 35rpx;
  font-weight: 800;
  margin-right: 20rpx;
}
